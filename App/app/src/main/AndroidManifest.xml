<?xml version="1.0" encoding="utf-8"?><!DOCTYPE manifest [<!ELEMENT manifest (uses-permission|application)*><!ATTLIST manifestxmlns:android CDATA #REQUIREDxmlns:tools CDATA #REQUIREDpackage CDATA #REQUIRED><!ELEMENT uses-permission (#PCDATA)><!ATTLIST uses-permissionandroid:name CDATA #REQUIRED><!ELEMENT application (activity)*><!ATTLIST applicationandroid:allowBackup CDATA #REQUIREDandroid:icon CDATA #REQUIREDandroid:label CDATA #REQUIREDandroid:supportsRtl CDATA #REQUIREDandroid:theme CDATA #REQUIREDtools:replace CDATA #REQUIRED><!ELEMENT activity (intent-filter|meta-data)*><!ATTLIST activityandroid:label CDATA #IMPLIEDandroid:name CDATA #REQUIREDandroid:parentActivityName CDATA #IMPLIEDandroid:screenOrientation CDATA #IMPLIEDandroid:theme CDATA #REQUIRED><!ELEMENT intent-filter (action|category)*><!ELEMENT action (#PCDATA)><!ATTLIST actionandroid:name CDATA #REQUIRED><!ELEMENT category (#PCDATA)><!ATTLIST categoryandroid:name CDATA #REQUIRED><!ELEMENT meta-data (#PCDATA)><!ATTLIST meta-dataandroid:name CDATA #REQUIREDandroid:value CDATA #REQUIRED>]>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="app.example.icas.integratedconcussionassessmentsystem">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:allowBackup">
        <activity
            android:name=".Scat3"
            android:label="SCAT 3"
            android:theme="@style/AppTheme.NoActionBar.Fullscreen" />
        <activity
            android:name=".scat3_instructions"
            android:label="SCAT 3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Posture_test2"
            android:label="Posture Test"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Homescreen"
            android:label="@string/title_activity_homescreen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Posturography"
            android:label="Posture Test"
            android:parentActivityName=".Homescreen"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="app.example.icas.integratedconcussionassessmentsystem.Homescreen" />
        </activity>
        <activity
            android:name=".Scat3_landing"
            android:label="SCAT3 Test"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".firsttimeform.background_form"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>